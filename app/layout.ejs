<!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#Wallet">
  Launch demo modal
</button>

<!-- Modal -->
<div class="modal fade" id="Wallet" tabindex="-1" aria-labelledby="WalletLabel" aria-hidden="true">
  <div class="modal-dialog modal-fullscreen-md-down">
    <div class="modal-content">
      <div class="modal-header flex-column ">
        <h5 class="modal-title w-100 text-center position-relative" id="WalletLabel">連結你的錢包 <button type="button" class="btn-close ms-auto d-inline-block position-absolute end-0" data-bs-dismiss="modal" aria-label="Close"></button></h5>
      </div>
      <div class="modal-body">
        <ul class="list-unstyled w-100 row row-cols-1 row-cols-lg-2 g-2">
          <li class="col ">
            <button type="button" class="btn border w-100 text-start d-flex align-items-center"><img class="w-16px me-4 d-block" src="./assets/images/logo/connect01.png" alt="connect01" /><p class="mb-0">MetaMask</p> </button>
          </li>
          <li class="col ">
            <button type="button" class="btn border w-100 text-start d-flex align-items-center"><img class="w-16px me-4 d-block" src="./assets/images/logo/connect02.png" alt="connect02" /><p class="mb-0">Coinbase Wallet</p> </button>
          </li>
            <li class="col ">
            <button type="button" class="btn border w-100 text-start d-flex align-items-center"><img class="w-16px me-4 d-block" src="./assets/images/logo/connect03.png" alt="connect03" /><p class="mb-0">WalletConnect</p> </button>
          </li>
            <li class="col ">
            <button type="button" class="btn border w-100 text-start d-flex align-items-center"><img class="w-16px me-4 d-block" src="./assets/images/logo/connect04.png" alt="connect04" /><p class="mb-0">Phantom</p> </button>
          </li>  
          <li class="col ">
            <button type="button" class="btn border w-100 text-start d-flex align-items-center"><img class="w-16px me-4 d-block" src="./assets/images/logo/connect05.png" alt="connect05" /><p class="mb-0">Glow</p> </button>
          </li>  
          <li class="col ">
            <button type="button" class="btn border w-100 text-start d-flex align-items-center"><img class="w-16px me-4 d-block" src="./assets/images/logo/connect06.png" alt="connect06" /><p class="mb-0">Fortmatic</p> </button>
          </li>
        </ul>
      </div>
      <div class="modal-footer d-flex justify-content-center border-0 p-0 p-lg-4 pt-lg-0">
      <a class="d-block btn btn-primary fs-6 text-white border border-black w-100 p-3 w-lg-30 m-0" >連結錢包</a>
      </div>
    </div>
  </div>
